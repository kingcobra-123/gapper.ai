# -----------------------------------------------------------------------------
# Frontend runtime env (all vars currently used by gapper.ai)
# -----------------------------------------------------------------------------

# Supabase client auth (public keys only)
VITE_SUPABASE_URL=https://YOUR_PROJECT_REF.supabase.co
VITE_SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY

# Terminal/backend API target
VITE_GAPPER_API_BASE_URL=http://100.76.85.102:8000
VITE_GAPPER_API_KEY=
VITE_GAPPER_API_TIMEOUT_MS=8000

# Premium gating
VITE_PREMIUM_GATE_ENABLED=false
NEXT_PUBLIC_PREMIUM_GATE_ENABLED=false

# Legacy auth-verify proxy (optional)
# Set to "off" to disable /api/auth/verify calls from AuthProvider.
VITE_AUTH_VERIFY_ENDPOINT=off
VITE_AUTH_API_PROXY_TARGET=http://127.0.0.1:8787

# Auth/profile behavior (optional)
VITE_ALLOW_PROFILE_UPSERT_FALLBACK=false

# Dev-server HMR override (optional)
# Leave empty unless you need a fixed websocket port behind a proxy/tunnel.
VITE_HMR_CLIENT_PORT=

# Optional local auth-server settings (used by `npm run auth:server`)
AUTH_SERVER_PORT=8787
AUTH_ALLOWED_ORIGINS=http://localhost:5173,http://127.0.0.1:5173

# Analytics (optional)
VITE_POSTHOG_API_KEY=
VITE_POSTHOG_API_HOST=https://us.i.posthog.com

# IMPORTANT:
# Never place SUPABASE_SERVICE_ROLE_KEY in frontend env files.

# Additional dev/test env keys referenced in scripts/tests
GAPPER_TEST_API_BASE_URL=
GAPPER_TEST_API_KEY=
GAPPER_TEST_POLYGON_API_KEY=
NEXT_PUBLIC_API_BASE_URL=
NEXT_PUBLIC_API_KEY=
NEXT_PUBLIC_API_TIMEOUT_MS=
NODE_ENV=
POLYGON_TOKEN=
PORT=
SKIP_GAPPER_WORKSPACE_GUARD=
SUPABASE_ANON_KEY=
SUPABASE_URL=
